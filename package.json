{
  "name": "grocery-store",
  "version": "2.2.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev:front": "pnpm --filter frontend dev",
    "dev:back": "pnpm --filter backend dev",
    "commit": "cz",
    "lint": "biome check --write --unsafe apps/**/src",
    "test": "vitest run --silent --reporter verbose",
    "test:watch": "vitest --silent --reporter verbose",
    "db:up": "docker compose -f docker-compose.dev.yml up -d",
    "db:down": "docker compose -f docker-compose.dev.yml down",
    "husky:install": "husky install",
    "husky:prepare": "yarn husky:install && chmod ug+x .husky/* && yarn husky:install",
    "prepare": "husky"
  },
  "dependencies": {
    "@biomejs/biome": "2.3.12",
    "@types/node": "^25.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^14.0.1",
    "typescript": "~5.9.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "commitizen": "^4.3.1"
  }
}
